# Frontend environment variables
# Copy this file to ".env" and adjust values for your environment.

# Base URL of the backend API (FastAPI). Default local backend is http://localhost:3001
REACT_APP_API_BASE_URL=http://localhost:3001

# Frontend origin (used by backend CORS config). Default React dev server is http://localhost:3000
# This is informational for developers; the backend should read its own FRONTEND_ORIGIN/.env.
# Keep this in sync with the backend's CORS allow_origins setting.
REACT_APP_FRONTEND_URL=http://localhost:3000

# Optional: upload limits and flags
# Max upload size in MB (client-side validation only)
REACT_APP_MAX_UPLOAD_MB=10

# Optional: React runtime flags
REACT_APP_NODE_ENV=development
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
